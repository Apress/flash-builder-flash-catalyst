<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:components="components.*" autoDrawBackground="false" xmlns:d="http://ns.adobe.com/fxg/2008/dt">
	<fx:Script>
		<![CDATA[
			import be.flexpert.webshop.valueObjects.Photo;
		]]>
	</fx:Script>
	<s:states>
		<s:State name="normal"/>
		<s:State name="hovered"/>
		<s:State name="selected"/>
	</s:states>
	<components:MyListItem photo="{data as Photo}" x="0" y="0">
	</components:MyListItem>
	<s:Rect d:userLabel="Item Highlight Rectangle" width="199" height="200" alpha.normal="0" alpha.hovered="0.3" alpha.selected="0.5" visible.hovered="false" visible.selected="false">
		<s:fill>
			<s:SolidColor color="0xCED7EE"/>
		</s:fill>
	</s:Rect>
</s:ItemRenderer>
